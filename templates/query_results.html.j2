{% extends "layout.html.j2" %}
{% block content %}
    {% for result in query_result %}
    {% for item in result %}
        <div class="content_container">
        {% for key, value in item.items() %}
            <div class="item_container">
                <h1>{{key}}</h1>
                <p>{{value}}</p>
            </div>
        {% endfor %}
        </div>
    {% endfor %}    
    {% endfor %}
{% endblock content %}